<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.Controller.MainController"
      spacing="10" alignment="CENTER">
    <Button text="Prosty Kalkulator" onAction="#handleEasyCalculator"/>
    <Button text="Zaawansowany Kalkulator" onAction="#handleHardCalculator"/>
    <Button text="Historia Obliczeń" onAction="#handleHistory"/>
    <Button text="Funkcje" onAction="#handleFunctions"/>
    <Button text="Zakończ" onAction="#handleExit"/>
</VBox>
